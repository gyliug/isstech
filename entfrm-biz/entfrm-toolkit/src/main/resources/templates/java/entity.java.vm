package ${packageName}.entity;

import com.baomidou.mybatisplus.annotation.TableId;
import com.baomidou.mybatisplus.annotation.TableName;
import com.entfrm.data.entity.BaseEntity;
import com.entfrm.base.annotation.Excel;
import com.fasterxml.jackson.annotation.JsonFormat;
import lombok.Data;
import lombok.EqualsAndHashCode;
import lombok.experimental.Accessors;
#if($table.tplCategory == 'tree' || $table.tplCategory == 'msub')
import com.baomidou.mybatisplus.annotation.TableField;
#end
#foreach ($import in $importList)
import ${import};
#end

/**
 * @author ${author}
 * @date ${datetime}
 *
 * @description ${functionName}对象 ${ClassName}
 */
@Data
@EqualsAndHashCode(callSuper = false)
@Accessors(chain = true)
@TableName("${tableName}")
public class ${ClassName} extends BaseEntity{
    private static final long serialVersionUID = 1L;

#foreach ($column in $columns)
#if(!$column.superColumn)
    /** $column.columnComment */
#if($column.isList == '1')
#set($parentheseIndex=$column.columnComment.indexOf("（"))
#if($parentheseIndex != -1)
#set($comment=$column.columnComment.substring(0, $parentheseIndex))
#else
#set($comment=$column.columnComment)
#end
#if($parentheseIndex != -1)
    @Excel(name = "${comment}", convertExp = "$column.convertExp()")
#elseif($column.javaType == 'Date')
    @JsonFormat(pattern = "yyyy-MM-dd")
    @Excel(name = "${comment}", width = 30, dateFormat = "yyyy-MM-dd")
#else
    @Excel(name = "${comment}")
#end
#end
#if($column.isPk == '1')
    @TableId
#end
    private $column.javaType $column.javaField;

#end
#end

#if($table.tplCategory == 'msub')
    /** $table.subTable.functionName信息 */
    @TableField(exist = false)
    private List<${subClassName}> ${subclassName}List;
#end

#if($table.tplCategory == 'tree')
    /**
    * 子菜单
    */
    @TableField(exist = false)
    private List<${ClassName}> children = new ArrayList<>();
#end
}
